{
    "version": "1.0.0-*",
    "webroot": "wwwroot",
    "exclude": [
        "wwwroot"
    ],
    "packExclude": 
    [
        "**.kproj",
        "**.user",
        "**.vspscc"
    ],
    "dependencies": 
    {
        "Microsoft.AspNet.Diagnostics": "1.0.0-beta4-12821",
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta4-11776",
        "Microsoft.AspNet.Mvc": "6.0.0-beta4-13554",
        "Microsoft.AspNet.Hosting": "1.0.0-beta4-11618",
        "Microsoft.AspNet.Http": "1.0.0-beta4-11362",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta4-11985",
        "Microsoft.AspNet.Server.Kestrel": "1.0.0-beta4-11536",
        /*
            ===============================================================
            Servers
        */
        /*
            Kestrel (libuv) => IIS express
        */
        "Kestrel": "1.0.0-beta4-11536",
        /*
            Kestrel (libuv) => IIS express
        */
        "Microsoft.AspNet.Server.IIS": "1.0.0-beta4-11565"
        /*
            NoWin.vNext
        */
        /*
            
        */
    },
    "commands": 
    {
        "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5001",
        "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5004"
    },
    "frameworks": 
    {
        "dnx451" : { },
        "dnxcore50": { }, 
        "aspnet50" : { }
    }
}
